package com.lec.basic.poly;

public class LGTV implements TV{

	public LGTV() {System.out.println("엘지티비 생성자 호출");}

	public void powerOn() {System.out.println("LGTV Power On");}
	
	public void powerOff() {System.out.println("LGTV Power Off");}
	
	public void volummUp() {System.out.println("LGTV Volumm Up");}
	
	public void volummDown() {System.out.println("LGTV Volumm Down");}
}
