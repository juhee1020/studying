package com.lec.basic.poly3;

public class AppleSpeaker implements Speaker{

	public AppleSpeaker() {System.out.println("==> AppleSpeaker 객체 생성");}
	
	@Override
	public void volummUp() {
		System.out.println("AppleSpeaker 볼륨업");
	}

	@Override
	public void volummDown() {
		System.out.println("AppleSpeaker 볼륨다운");
	}

	
}
