<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="collectionBean" class="com.lec.basic.collection.CollectionBean">

		<property name="addressList">
			<list>
				<value>서울시 서초구 이젠 캠퍼스1</value>
				<value>서울시 강남구 이젠 캠퍼스2</value>
			</list>
		</property>

		<property name="addressSet">
			<set value-type="java.lang.String"><!-- set은 value-type줘야함 -->
				<value>경기도 신한카드 1</value>
				<value>경기도 신한카드 2</value>
				<value>경기도 신한카드 1</value>
			</set>
		</property>
	
		<property name="addMap">
			<map>
				<entry>
					<key><value>홍길동</value></key>
					<value>서울</value>
				</entry>
				<entry>
					<key><value>홍길순</value></key>
					<value>부산</value>
				</entry>
				<entry>
					<key><value>홍길자</value></key>
					<value>대전</value>
				</entry>
			</map>
		</property>

		<property name="addProps">
			<props>
				<prop key="user">scott</prop>
				<prop key="pass">tiger</prop>
			</props>
		</property>

	</bean>
</beans>